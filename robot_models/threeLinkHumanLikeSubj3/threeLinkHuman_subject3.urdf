<robot name="threeLinkHuman_subject3">
    <link name="foot">
        <inertial>
            <mass value="3.470"/>
            <origin xyz="0 0 0.0255" rpy="0 0 0" />
            <inertia ixx="0.010" iyy="0.018" izz="0.027" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
			<origin xyz="0 0 0.0255" rpy="0 0 0" />
            <geometry>
                <box size="0.244 0.183 0.051"/>
