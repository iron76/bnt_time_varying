        </visual>

        <collision>
			<<origin xyz="0 0 0.0255" rpy="0 0 0" />
            <geometry>
                <box size="0.244 0.183 0.051"/>
            </geometry>
        </collision>
    </link>
    
    <link name="leg">
        <inertial>
            <mass value="32.606"/>
            <origin xyz="0 0 0.437" rpy="0 0 0" />
            <inertia ixx="2.191" iyy="2.115" izz="0.154" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
            <origin xyz="0 0 0.437" rpy="0 0 0" />
            <geometry>
                <box size="0.120 0.206 0.874"/>
            </geometry>
        </visual>

        <collision>
            <origin xyz="0 0 0.437" rpy="0 0 0" />
            <geometry>
                <box size="0.120 0.206 0.874"/>
            </geometry>
        </collision>
    </link>
    
    <link name="torso">
        <inertial>
            <mass value="27.455"/>
            <origin xyz="0 0 0.2315" rpy="0 0 0" />
            <inertia ixx="0.735" iyy="0.524" izz="0.277" ixy="0" ixz="0" iyz="0"/>
