        </collision>
    </link>
    
    <link name="leg">
        <inertial>
            <mass value="30.6574"/>
            <origin xyz="0 0 0.39988" rpy="0 0 0" />
            <inertia ixx="1.7774" iyy="1.6656" izz="0.17493" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
            <origin xyz="0 0 0.39988" rpy="0 0 0" />
            <geometry>
                <box size="0.11115 0.23689 0.79975"/>
            </geometry>
        </visual>

        <collision>
            <origin xyz="0 0 0.39988" rpy="0 0 0" />
            <geometry>
                <box size="0.11115 0.23689 0.79975"/>
            </geometry>
        </collision>
    </link>
    
    <link name="torso">
        <inertial>
            <mass value="28.5522"/>
            <origin xyz="0 0 0.24592" rpy="0 0 0" />
            <inertia ixx="0.88179" iyy="0.60497" izz="0.33561" ixy="0" ixz="0" iyz="0"/>
