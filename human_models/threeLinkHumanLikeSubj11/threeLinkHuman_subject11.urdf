        </visual>

        <collision>
			<origin xyz="0 0 0.035272" rpy="0 0 0" />
            <geometry>
                <box size="0.25481 0.24345 0.070543"/>
            </geometry>
        </collision>
    </link>
    
    <link name="leg">
        <inertial>
            <mass value="35.8194"/>
            <origin xyz="0 0 0.40222" rpy="0 0 0" />
            <inertia ixx="2.1308" iyy="1.9801" izz="0.24765" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
            <origin xyz="0 0 0.40222" rpy="0 0 0" />
            <geometry>
                <box size="0.1274 0.25833 0.80444"/>
            </geometry>
        </visual>

        <collision>
            <origin xyz="0 0 0.40222" rpy="0 0 0" />
            <geometry>
                <box size="0.1274 0.25833 0.80444"/>
            </geometry>
        </collision>
    </link>
    
    <link name="torso">
        <inertial>
            <mass value="32.8776"/>
            <origin xyz="0 0 0.26928" rpy="0 0 0" />
            <inertia ixx="1.1383" iyy="0.83912" izz="0.38816" ixy="0" ixz="0" iyz="0"/>
