<robot name="threeLinkHuman">
    <link name="foot">
        <inertial>
            <mass value="3.0614"/>
            <origin xyz="0 0 0.030677" rpy="0 0 0" />
            <inertia ixx="0.0051743" iyy="0.014563" izz="0.017816" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
			<origin xyz="0 0 0.030677" rpy="0 0 0" />
            <geometry>
                <box size="0.23091 0.12852 0.061354"/>
            </geometry>
        </visual>

        <collision>
			<origin xyz="0 0 0.030677" rpy="0 0 0" />
            <geometry>
                <box size="0.23091 0.12852 0.061354"/>
            </geometry>
        </collision>
    </link>
    
    <link name="leg">
        <inertial>
            <mass value="25.4904"/>
            <origin xyz="0 0 0.39925" rpy="0 0 0" />
            <inertia ixx="1.4119" iyy="1.3827" izz="0.085757" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
            <origin xyz="0 0 0.39925" rpy="0 0 0" />
            <geometry>
                <box size="0.11545 0.16444 0.79851"/>
            </geometry>
        </visual>

        <collision>
            <origin xyz="0 0 0.39925" rpy="0 0 0" />
            <geometry>
                <box size="0.11545 0.16444 0.79851"/>
            </geometry>
        </collision>
    </link>
    
    <link name="torso">
        <inertial>
            <mass value="33.1436"/>
            <origin xyz="0 0 0.26942" rpy="0 0 0" />
            <inertia ixx="1.0792" iyy="0.83873" izz="0.31411" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
            <origin xyz="0 0 0.26942" rpy="0 0 0" />
            <geometry>
                <box size="0.11545 0.31686 0.53883"/>
            </geometry>
        </visual>

        <collision>
            <origin xyz="0 0 0.26942" rpy="0 0 0" />
            <geometry>
                <box size="0.11545 0.31686 0.53883"/>
            </geometry>
        </collision>
    </link>
        
    <joint name="ankle" type="revolute">
        <origin xyz="-0.057727 0 0.061354" rpy="0 0 0"/>
        <parent link="foot"/>
        <child link="leg"/>
        <dynamics damping="0.1" friction="0.0"/>
        <limit effort="30" velocity="1.0" lower="-10.0" upper="10.0" />
        <axis xyz="0 1 0" />
    </joint>
    
    <joint name="hip" type="revolute">
        <origin xyz="0 0 0.79851" rpy="0 0 0"/>
        <parent link="leg"/>
        <child link="torso"/>
        <dynamics damping="0.1" friction="0.0"/>
        <limit effort="30" velocity="1.0" lower="-10.0" upper="10.0" />
        <axis xyz="0 1 0" />
    </joint>

</robot>
