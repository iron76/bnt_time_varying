        </collision>
    </link>
    
    <link name="leg">
        <inertial>
            <mass value="35.397"/>
            <origin xyz="0 0 0.4231" rpy="0 0 0" />
            <inertia ixx="2.3097" iyy="2.153" izz="0.23826" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
            <origin xyz="0 0 0.4231" rpy="0 0 0" />
            <geometry>
                <box size="0.11753 0.25877 0.84621"/>
            </geometry>
        </visual>

        <collision>
            <origin xyz="0 0 0.4231" rpy="0 0 0" />
            <geometry>
                <box size="0.11753 0.25877 0.84621"/>
            </geometry>
        </collision>
    </link>
    
    <link name="torso">
        <inertial>
            <mass value="28.3864"/>
            <origin xyz="0 0 0.24775" rpy="0 0 0" />
            <inertia ixx="0.87788" iyy="0.61344" izz="0.32979" ixy="0" ixz="0" iyz="0"/>
