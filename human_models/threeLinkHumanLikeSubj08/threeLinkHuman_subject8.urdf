        <child link="leg"/>
        <dynamics damping="0.1" friction="0.0"/>
        <limit effort="30" velocity="1.0" lower="-10.0" upper="10.0" />
        <axis xyz="0 1 0" />
    </joint>
    
    <joint name="hip" type="revolute">
        <origin xyz="0 0 0.80859" rpy="0 0 0"/>
        <parent link="leg"/>
        <child link="torso"/>
        <dynamics damping="0.1" friction="0.0"/>
        <limit effort="30" velocity="1.0" lower="-10.0" upper="10.0" />
        <axis xyz="0 1 0" />
    </joint>

</robot>
