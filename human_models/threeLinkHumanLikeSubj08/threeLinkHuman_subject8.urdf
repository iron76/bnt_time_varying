    <link name="leg">
        <inertial>
            <mass value="41.5446"/>
            <origin xyz="0 0 0.40454" rpy="0 0 0" />
            <inertia ixx="2.5568" iyy="2.3269" izz="0.35106" ixy="0" ixz="0" iyz="0"/>
