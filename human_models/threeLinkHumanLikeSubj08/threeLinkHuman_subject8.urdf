<robot name="threeLinkHuman">
    <link name="foot">
        <inertial>
            <mass value="6.7669"/>
            <origin xyz="0 0 0.032373" rpy="0 0 0" />
            <inertia ixx="0.050811" iyy="0.041583" izz="0.087667" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
			<origin xyz="0 0 0.032373" rpy="0 0 0" />
            <geometry>
                <box size="0.26372 0.29311 0.064746"/>
            </geometry>
        </visual>

        <collision>
			<origin xyz="0 0 0.032373" rpy="0 0 0" />
            <geometry>
                <box size="0.26372 0.29311 0.064746"/>
            </geometry>
        </collision>
    </link>
    
    <link name="leg">
        <inertial>
            <mass value="41.4108"/>
            <origin xyz="0 0 0.40429" rpy="0 0 0" />
            <inertia ixx="2.541" iyy="2.3162" izz="0.34476" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
            <origin xyz="0 0 0.40429" rpy="0 0 0" />
            <geometry>
                <box size="0.13186 0.28726 0.80859"/>
            </geometry>
        </visual>

        <collision>
            <origin xyz="0 0 0.40429" rpy="0 0 0" />
            <geometry>
                <box size="0.13186 0.28726 0.80859"/>
            </geometry>
        </collision>
    </link>
    
    <link name="torso">
        <inertial>
            <mass value="35.2102"/>
            <origin xyz="0 0 0.27249" rpy="0 0 0" />
            <inertia ixx="1.2568" iyy="0.92246" izz="0.43635" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
            <origin xyz="0 0 0.27249" rpy="0 0 0" />
            <geometry>
                <box size="0.13186 0.36239 0.54497"/>
