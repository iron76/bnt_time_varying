<robot name="threeLinkHuman">
    <link name="foot">
        <inertial>
            <mass value="4.3736"/>
            <origin xyz="0 0 0.027008" rpy="0 0 0" />
            <inertia ixx="0.014093" iyy="0.018053" izz="0.03002" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
			<origin xyz="0 0 0.027008" rpy="0 0 0" />
            <geometry>
                <box size="0.21591 0.18908 0.054016"/>
            </geometry>
        </visual>

        <collision>
			<origin xyz="0 0 0.027008" rpy="0 0 0" />
            <geometry>
                <box size="0.21591 0.18908 0.054016"/>
            </geometry>
        </collision>
    </link>
    
    <link name="leg">
        <inertial>
            <mass value="32.0573"/>
            <origin xyz="0 0 0.37067" rpy="0 0 0" />
            <inertia ixx="1.5771" iyy="1.4993" izz="0.14009" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
            <origin xyz="0 0 0.37067" rpy="0 0 0" />
            <geometry>
                <box size="0.10795 0.20196 0.74134"/>
            </geometry>
        </visual>

        <collision>
            <origin xyz="0 0 0.37067" rpy="0 0 0" />
            <geometry>
                <box size="0.10795 0.20196 0.74134"/>
            </geometry>
        </collision>
    </link>
    
    <link name="torso">
        <inertial>
            <mass value="35.5992"/>
            <origin xyz="0 0 0.25813" rpy="0 0 0" />
            <inertia ixx="1.0984" iyy="0.82527" izz="0.34224" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
            <origin xyz="0 0 0.25813" rpy="0 0 0" />
            <geometry>
                <box size="0.10795 0.32204 0.51627"/>
