        </collision>
    </link>
    
    <link name="leg">
        <inertial>
            <mass value="28.0587"/>
            <origin xyz="0 0 0.40464" rpy="0 0 0" />
            <inertia ixx="1.6105" iyy="1.5635" izz="0.11119" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
            <origin xyz="0 0 0.40464" rpy="0 0 0" />
            <geometry>
                <box size="0.11708 0.18398 0.80928"/>
            </geometry>
        </visual>

        <collision>
            <origin xyz="0 0 0.40464" rpy="0 0 0" />
            <geometry>
                <box size="0.11708 0.18398 0.80928"/>
            </geometry>
        </collision>
    </link>
    
    <link name="torso">
        <inertial>
            <mass value="32.7694"/>
            <origin xyz="0 0 0.25837" rpy="0 0 0" />
            <inertia ixx="1.0384" iyy="0.76659" izz="0.34666" ixy="0" ixz="0" iyz="0"/>
