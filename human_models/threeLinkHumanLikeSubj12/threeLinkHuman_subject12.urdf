        </collision>
    </link>
    
    <link name="leg">
        <inertial>
            <mass value="28.0108"/>
            <origin xyz="0 0 0.40516" rpy="0 0 0" />
            <inertia ixx="1.6114" iyy="1.5645" izz="0.11049" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
            <origin xyz="0 0 0.40516" rpy="0 0 0" />
            <geometry>
                <box size="0.11676 0.18359 0.81032"/>
            </geometry>
        </visual>

        <collision>
            <origin xyz="0 0 0.40516" rpy="0 0 0" />
            <geometry>
                <box size="0.11676 0.18359 0.81032"/>
            </geometry>
        </collision>
    </link>
    
    <link name="torso">
        <inertial>
            <mass value="32.724"/>
            <origin xyz="0 0 0.25804" rpy="0 0 0" />
            <inertia ixx="1.0356" iyy="0.76346" izz="0.34644" ixy="0" ixz="0" iyz="0"/>
