<robot name="threeLinkHuman">
    <link name="foot">
        <inertial>
            <mass value="4.0649"/>
            <origin xyz="0 0 0.030677" rpy="0 0 0" />
            <inertia ixx="0.016749" iyy="0.018721" izz="0.03292" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
			<origin xyz="0 0 0.030677" rpy="0 0 0" />
            <geometry>
                <box size="0.22694 0.21373 0.061353"/>
            </geometry>
        </visual>

        <collision>
			<origin xyz="0 0 0.030677" rpy="0 0 0" />
            <geometry>
                <box size="0.22694 0.21373 0.061353"/>
            </geometry>
        </collision>
    </link>
    
    <link name="leg">
        <inertial>
            <mass value="32.7484"/>
            <origin xyz="0 0 0.4457" rpy="0 0 0" />
            <inertia ixx="2.3218" iyy="2.2036" izz="0.18846" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
            <origin xyz="0 0 0.4457" rpy="0 0 0" />
            <geometry>
                <box size="0.11347 0.23703 0.8914"/>
            </geometry>
        </visual>

        <collision>
            <origin xyz="0 0 0.4457" rpy="0 0 0" />
            <geometry>
                <box size="0.11347 0.23703 0.8914"/>
            </geometry>
        </collision>
    </link>
    
    <link name="torso">
        <inertial>
            <mass value="20.1525"/>
            <origin xyz="0 0 0.21781" rpy="0 0 0" />
            <inertia ixx="0.46829" iyy="0.3403" izz="0.17124" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
            <origin xyz="0 0 0.21781" rpy="0 0 0" />
            <geometry>
                <box size="0.11347 0.29848 0.43561"/>
