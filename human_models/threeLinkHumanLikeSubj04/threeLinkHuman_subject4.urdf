<robot name="threeLinkHuman">
    <link name="foot">
        <inertial>
            <mass value="4.271"/>
            <origin xyz="0 0 0.033403" rpy="0 0 0" />
            <inertia ixx="0.010709" iyy="0.025553" izz="0.033085" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
			<origin xyz="0 0 0.033403" rpy="0 0 0" />
            <geometry>
                <box size="0.25948 0.16008 0.066806"/>
            </geometry>
        </visual>

        <collision>
			<origin xyz="0 0 0.033403" rpy="0 0 0" />
            <geometry>
                <box size="0.25948 0.16008 0.066806"/>
            </geometry>
        </collision>
    </link>
    
    <link name="leg">
        <inertial>
            <mass value="37.5187"/>
            <origin xyz="0 0 0.46248" rpy="0 0 0" />
            <inertia ixx="2.804" iyy="2.7276" izz="0.18164" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
            <origin xyz="0 0 0.46248" rpy="0 0 0" />
            <geometry>
                <box size="0.12974 0.20314 0.92497"/>
            </geometry>
        </visual>

        <collision>
            <origin xyz="0 0 0.46248" rpy="0 0 0" />
            <geometry>
                <box size="0.12974 0.20314 0.92497"/>
            </geometry>
        </collision>
    </link>
    
    <link name="torso">
        <inertial>
            <mass value="35.2685"/>
            <origin xyz="0 0 0.25066" rpy="0 0 0" />
            <inertia ixx="1.1034" iyy="0.78809" izz="0.41431" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
            <origin xyz="0 0 0.25066" rpy="0 0 0" />
            <geometry>
                <box size="0.12974 0.35233 0.50131"/>
