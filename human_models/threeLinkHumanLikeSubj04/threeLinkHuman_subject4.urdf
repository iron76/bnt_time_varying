            <origin xyz="0 0 0.25066" rpy="0 0 0" />
            <geometry>
                <box size="0.12974 0.35233 0.50131"/>
            </geometry>
        </collision>
    </link>
        
    <joint name="ankle" type="revolute">
        <origin xyz="-0.064871 0 0.066806" rpy="0 0 0"/>
