<robot name="threeLinkHuman">
    <link name="foot">
        <inertial>
            <mass value="4.2949"/>
            <origin xyz="0 0 0.033723" rpy="0 0 0" />
            <inertia ixx="0.010667" iyy="0.025807" izz="0.033217" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
			<origin xyz="0 0 0.033723" rpy="0 0 0" />
            <geometry>
                <box size="0.25991 0.15891 0.067446"/>
            </geometry>
        </visual>

        <collision>
			<origin xyz="0 0 0.033723" rpy="0 0 0" />
            <geometry>
                <box size="0.25991 0.15891 0.067446"/>
            </geometry>
        </collision>
    </link>
    
    <link name="leg">
        <inertial>
            <mass value="37.3011"/>
            <origin xyz="0 0 0.46065" rpy="0 0 0" />
            <inertia ixx="2.7654" iyy="2.691" izz="0.17943" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
            <origin xyz="0 0 0.46065" rpy="0 0 0" />
            <geometry>
                <box size="0.12996 0.20207 0.92131"/>
            </geometry>
        </visual>

        <collision>
            <origin xyz="0 0 0.46065" rpy="0 0 0" />
            <geometry>
                <box size="0.12996 0.20207 0.92131"/>
            </geometry>
        </collision>
    </link>
    
    <link name="torso">
        <inertial>
            <mass value="35.4327"/>
            <origin xyz="0 0 0.25057" rpy="0 0 0" />
            <inertia ixx="1.1092" iyy="0.79141" izz="0.41758" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <visual>
            <origin xyz="0 0 0.25057" rpy="0 0 0" />
            <geometry>
                <box size="0.12996 0.35289 0.50114"/>
